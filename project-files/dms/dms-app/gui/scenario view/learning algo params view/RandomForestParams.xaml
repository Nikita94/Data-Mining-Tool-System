<UserControl x:Class="dms.gui.scenario_view.RandomForestParams"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:dms.gui.scenario_view"
      mc:Ignorable="d" 
      xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" d:DesignWidth="300" Height="59.672">

    <Grid Margin="0,0,0,10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Content="{Binding ParamsName[0]}"/>
        <ComboBox ItemsSource="{Binding AlgosTypesList}" SelectedItem="{Binding AlgoType}" Grid.Row="0" Grid.Column="1" Margin="0,0,0,10"/>
        <Label Grid.Row="1" Grid.Column="0" Content="{Binding ParamsName[1]}"/>
        <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding PartOfSel}" Margin="0,0,0,10"/>
    </Grid>
</UserControl>